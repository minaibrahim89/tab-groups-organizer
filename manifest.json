{
    "manifest_version": 3,
    "name": "Tab Group Snapshot Manager",
    "version": "1.1",
    "description": "Create and restore snapshots of tab groups",
    "permissions": [
      "tabs",
      "tabGroups",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  